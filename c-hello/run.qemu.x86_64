#!/bin/sh

if test ! -f "workdir/build/c-hello_qemu-x86_64"; then
    echo "No kernel file workdir/build/c-hello_qemu-x86_64." 1>&2
    echo "Did you run ./build.qemu.x86_64 ?" 1>&2
    exit 1
fi

qemu-system-x86_64 \
    -nographic \
    -m 8 \
    -cpu max \
    -kernel workdir/build/c-hello_qemu-x86_64
