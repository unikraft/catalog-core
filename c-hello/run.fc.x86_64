#!/bin/sh

if test ! -f "workdir/build/c-hello_fc-x86_64"; then
    echo "No kernel file workdir/build/c-hello_fc-x86_64." 1>&2
    echo "Did you run ./build.fc.x86_64 ?" 1>&2
    exit 1
fi

rm -f firecracker.socket
firecracker-x86_64 \
        --api-sock firecracker.socket \
        --config-file fc.x86_64.json
