#!/bin/sh

if test ! -f "workdir/build/cpp-hello_fc-arm64"; then
    echo "No kernel file workdir/build/cpp-hello_fc-arm64." 1>&2
    echo "Did you run ./build.fc.arm64 ?" 1>&2
    exit 1
fi

rm -f firecracker.socket
firecracker-aarch64 \
        --api-sock firecracker.socket \
        --config-file fc.arm64.json
